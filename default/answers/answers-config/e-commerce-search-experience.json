{
  "$schema": "https://schema.yext.com/config/answers/answers-config/v1",
  "$id": "e-commerce-search-experience",
  "name": "E-commerce Search Experience",
  "supportedLocales": [],
  "countryRestrictions": [],
  "verticals": {
    "faqs": {
      "entityTypes": [
        "faq"
      ],
      "name": "FAQs",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "keywords": {
          "textSearch": true
        },
        "question": {
          "semanticTextSearch": true
        }
      },
      "sortBys": [],
      "source": "KNOWLEDGE_MANAGER"
    },
    "jobs": {
      "entityTypes": [
        "job"
      ],
      "name": "Jobs",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "builtin.location": {
          "nlpFilter": true
        },
        "employmentType": {
          "nlpFilter": true,
          "textSearch": true
        },
        "keywords": {
          "textSearch": true
        },
        "name": {
          "textSearch": true
        }
      },
      "sortBys": [],
      "source": "KNOWLEDGE_MANAGER"
    },
    "locations": {
      "entityTypes": [
        "location"
      ],
      "name": "Locations",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "builtin.location": {
          "nlpFilter": true
        },
        "keywords": {
          "textSearch": true
        },
        "name": {
          "textSearch": true
        }
      },
      "sortBys": [],
      "source": "KNOWLEDGE_MANAGER"
    },
    "products": {
      "entityTypes": [
        "product"
      ],
      "name": "Products",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "name": {
          "semanticTextSearch": true
        }
      },
      "sortBys": [],
      "source": "KNOWLEDGE_MANAGER"
    }
  },
  "synonyms": {
    "oneWay": [],
    "synonymSet": [
      [
        "product",
        "service",
        "offering"
      ],
      [
        "return",
        "exchange",
        "replace"
      ]
    ],
    "normalization": []
  },
  "localizations": {},
  "additionalSearchablePlaces": [],
  "querySuggestions": {
    "disablePopularQueries": false,
    "verticalPromptsOnEmpty": false,
    "popularQueriesBlacklistedTerms": [],
    "universalPrompts": [
      "see all products",
      "locations",
      "do you offer free shipping",
      "open job opportunities"
    ],
    "verticalPrompts": {
      "faqs": [
        "[[name]]"
      ],
      "jobs": [
        "[[name]]"
      ],
      "locations": [
        "Locations near [[address.city]]",
        "Locations near [[address.region]]"
      ],
      "products": [
        "[[name]]"
      ]
    }
  }
}